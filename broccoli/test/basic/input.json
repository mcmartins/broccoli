{
  "jobName": "Job1",
  "jobDescription": "Job1 Test using echo and sleep",
  "workingDir": "/tmp/",
  "timeout": 15,
  "tasks": [
    {
      "taskName": "T1",
      "taskDescription": "T1 Description",
      "wait": false,
      "timeout": 10,
      "commands": [
        "echo '1 - Were fine too!'; sleep 10;"
      ],
      "children": [
        {
          "taskName": "T1.1",
          "taskDescription": "T1.1 Description",
          "wait": false,
          "timeout": 10,
          "commands": [
            "echo '1.1 - Were fine!'; sleep 4;",
            "echo '1.1 - Were fine too!'; sleep 5;"
          ]
        }
      ]
    },
    {
      "taskName": "T2",
      "taskDescription": "T2 Description. This should finish first and terminate the process.",
      "wait": false,
      "timeout": 10,
      "commands": [
        "echo '2 - Were fine!'; sleep 3;",
        "echo '2 - Were fine too!'; sleep 2;"
      ]
    }
  ]
}
