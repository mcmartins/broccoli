{
  "jobName": "Job1",
  "jobDescription": "teste",
  "workingDir": "/tmp/autonomous",
  "timeout": 10000,
  "tasks": [
    {
      "taskName": "T1",
      "taskDescription": "Consequences",
      "wait": false,
      "failTolerant": true,
      "commands": [
        "../LADR-2009-11A/bin/prover9 -f Theory.in > Consequences.out"
      ],
      "children": [
        {
          "taskName": "T1.1",
          "taskDescription": "For each consequence found, replace in original theory and Run prover/mace",
          "wait": false,
          "failTolerant": true,
          "preparation": {
            "filterFile": "Consequences.out",
            "pattern": "^given.*?T.*?:\\s*\\d+([^.\\#]*.)",
            "writeFile": "New-Theory-*.in",
            "placeholder": "$placeholder",
            "copy": true
          },
          "commands": [
            "../LADR-2009-11A/bin/prover9 -f $file > $file_prover.out",
            "../LADR-2009-11A/bin/mace4 -f $file > $file_mace.out"
          ],
          "children": [
            {
              "taskName": "T1.1.1",
              "taskDescription": "For each output check for proofs prover/mace",
              "wait": false,
              "commands": [
                "grep \"THEOREM PROVED\" -R --exclude=\\*{json,log} ."
              ]
            }
          ]
        }
      ]
    }
  ]
}