{
  "jobName": "Job Name",
  "jobDescription": "Job Description",
  "workingDir": "/media/sf_GitHub/broccoli/broccoli",
  "timeout": 10,
  "verbose": false,
  "tasks": [{
    "taskName": "Task 1 Name",
    "taskDescription": "Task 1 Description",
    "execute":{
      "commands": ["prover9 -f Axioms.in > Axioms.out"]
    },
    "guidance": [{
      "taskName": "Task 1.1 Name",
      "taskDescription": "Task 1.1 Description",
      "execute": {
        "commands": ["prover9 -f From_Axioms.in > From_Axioms.out", "mace4 -f From_Axioms.in > From_Axioms.out"]
        "forEach": {
          "select": "line",
          "in": "Axioms.out",
          "matching": "^given.*?:\s*\d+([^.\#]*)[\#[^.]*]?\.\s*\[.*?\]\.",
          "do": {
           "write": "From_Axioms.in",
           "replace": "$placeholder",
          }
        }
      }
    }]
  }]
}