{
  "jobName": "Job1",
  "jobDescription": "",
  "workingDir": "/tmp",
  "timeout": 100,
  "verbose": true,
  "tasks": [
    {
      "taskName": "T1",
      "taskDescription": "Consequences",
      "wait": false,
      "timeout": 10,
      "preparation": {},
      "commands": [
        "prover9 -f Axioms.in > Axioms.out"
      ],
      "guidance": [
        {
          "taskName": "T1.1",
          "taskDescription": "For each consequence found, replace in original theory and Run prover/mace",
          "wait": false,
          "timeout": 10,
          "preparation": {
            "filterFile": "Axioms.out",
            "pattern": "^given.*?:\\s*\\d+([^.\\#]*)[\\#[^.]*]?\\.\\s*\\[.*?\\]\\.",
            "writeFile": "From_Axioms.in",
            "placeholder": "$placeholder",
            "copy": true
          },
          "commands": [
            "prover9 -f $file > $file_prover.out",
            "mace4 -f $file > $file_prover.out"
          ],
          "guidance": []
        }
      ]
    }
  ]
}