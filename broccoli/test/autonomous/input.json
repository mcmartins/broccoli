{
  "jobName": "Job1",
  "jobDescription": "teste",
  "workingDir": "/media/sf_GitHub/broccoli/broccoli/test/autonomous/",
  "timeout": 100,
  "tasks": [
    {
      "taskName": "T1",
      "taskDescription": "Consequences",
      "wait": false,
      "timeout": 10,
      "commands": [
        "prover9 -f /media/sf_GitHub/broccoli/broccoli/test/autonomous/Theory.in > /media/sf_GitHub/broccoli/broccoli/test/autonomous/Consequences.out"
      ],
      "children": [
        {
          "taskName": "T1.1",
          "taskDescription": "For each consequence found, replace in original theory and Run prover/mace",
          "wait": false,
          "timeout": 10,
          "preparation": {
            "filterFile": "/media/sf_GitHub/broccoli/broccoli/test/autonomous/Consequences.out",
            "pattern": "^given.*?T.*?:\\s*\\d+([^.\\#]*.)",
            "writeFile": "/media/sf_GitHub/broccoli/broccoli/test/autonomous/New-Theory-*.in",
            "placeholder": "$placeholder",
            "copy": true
          },
          "commands": [
            "prover9 -f $file > $file_prover.out",
            "mace4 -f $file > $file_mace.out"
          ]
        }
      ]
    }
  ]
}