{
  "jobName": "Name",
  "jobDescription": "Description",
  "workingDir": "/tmp/autonomous/",
  "timeout": 10,
  "verbose": false,
  "tasks": [{
    "taskName": "T1",
    "taskDescription": "Run Prover9 to generate consequences and find new proofs",
    "command": "prover9 -f Basic.in > Basic_prover.out",
    "guidance": [{
      "taskName": "T1.1",
      "taskDescription": "Run Prover and Mace for each line produced on previous task and look for new theorems",
      "execute": {
        "forEach": {
          "select": "file",
          "in": "/tmp/autonomous/",
          "matching": "*.in"
        },
        "do": {
          
          
        },
        "commands": ["prover9 -f $file > $file_prover.out", "mace4 -f $file > $file_mace.out"]
      }
    }]
  }]
}